<settings>
	<servers>
		<server>
			<id>codeartifact</id>
			<username>aws</username>
			<password>${env.CODEARTIFACT_AUTH_TOKEN}</password>
		</server>
	</servers>
	<profiles>
		<profile>
			<id>codeartifact</id>
				<activation>
					<activeByDefault>true</activeByDefault>
				</activation>
			<repositories>
				<repository>
					<id>${env.CODEARTIFACT_DOMAIN_REPO}</id>
					<url>${env.CODEARTIFACT_URL}</url>
				</repository>
			</repositories>
		</profile>
	</profiles>
</settings>
